package escuela.aplicacion

class Usuario {

	String nombre
	String apellidoPaterno
	String apellidoMaterno
	String password
	String numeroCuenta
	String correo
	String telefono

    static constraints = {
	numeroCuenta size: 5..15, blank: false, unique: true
        password size: 5..15, blank: false
        email correo: true, blank: false
    }
}
